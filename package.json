{
  "name": "vscode-trello",
  "displayName": "Trello",
  "description": "Get Trello cards, lists, and boards inside of VS Code.",
  "version": "0.0.1",
  "publisher": "Ho-Wan",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:trello.login",
    "onCommand:trello.getCard",
    "onCommand:trello.getBoards",
    "onCommand:trello.getLists",
    "onCommand:trello.getCards",
    "onCommand:trello.sayHello"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "trello.login",
        "title": "Trello: login"
      },
      {
        "command": "trello.getCard",
        "title": "Trello: get specific card"
      },
      {
        "command": "trello.getBoards",
        "title": "Trello: get boards"
      },
      {
        "command": "trello.getLists",
        "title": "Trello: get lists from board"
      },
      {
        "command": "trello.getCards",
        "title": "Trello: get cards from list"
      },
      {
        "command": "trello.sayHello",
        "title": "Hello Trello"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/oauth": "^0.9.1",
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "http": "0.0.0",
    "oauth": "^0.9.15",
    "trello": "^0.9.0",
    "url": "^0.11.0"
  }
}
