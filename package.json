{
  "name": "vscode-trello",
  "displayName": "Trello",
  "description": "Get Trello cards, lists, and boards inside of VS Code.",
  "version": "0.0.1",
  "publisher": "Ho-Wan",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:testView",
    "onCommand:trello.test",
    "onCommand:trello.setKey",
    "onCommand:trello.setToken",
    "onCommand:trello.getBoards",
    "onCommand:trello.getCard",
    "onCommand:trello.showCardText"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "trello.test",
        "title": "Trello: test"
      },
      {
        "command": "trello.setKey",
        "title": "Trello: set Key"
      },
      {
        "command": "trello.setToken",
        "title": "Trello: set token"
      },
      {
        "command": "trello.getBoards",
        "title": "Trello: get boards"
      },
      {
        "command": "trello.getCard",
        "title": "Trello: get card"
      },
      {
        "command": "trello.showCard",
        "title": "Trello: show card"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "view-trello",
          "title": "View Trello",
          "icon": "images/T-D.png"
        }
      ]
    },
    "views": {
      "view-trello": [
        {
					"id": "testView",
					"name": "Test View"
				},
        {
          "id": "trello-cards",
          "name": "Trello Cards"
        },
        {
          "id": "trello-list",
          "name": "Trello Lists"
        },
        {
          "id": "trello-boards",
          "name": "Trello Boards"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "event-stream": "^3.3.4"
  }
}
